{
  "name": "vue-simple-confirm",
  "version": "1.0.3",
  "description": "Simple popup component for Vue 3",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types",
    "typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit",
    "prepare": "husky install"
  },
  "keywords": [
    "dialog",
    "simple",
    "vue",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/listennn08/vue-simple-dialog"
  },
  "homepage": "https://github.com/listennn08/vue-simple-dialog",
  "bugs": "https://github.com/listennn08/vue-simple-dialog/issues",
  "author": "matt",
  "license": "MIT",
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@types/node": "^18.11.17",
    "@vue/tsconfig": "^0.1.3",
    "husky": "^8.0.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.18"
  },
  "peerDependencies": {
    "vue": "^3.x"
  }
}
